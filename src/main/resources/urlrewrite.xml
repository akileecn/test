<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <condition type="parameter" name="hello">^true$</condition>
        <from>/test/(.*)</from>
        <to>/hello</to>
    </rule>
</urlrewrite>